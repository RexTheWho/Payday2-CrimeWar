	<state name="fps/throw_projectile" type="mix" segment="base" speed="1.0">
		<from	name="fps/throw_projectile_start" blend="1"/>
		<from	name="fps/throw_projectile_idle" blend="1"/>
		<from	name="fps/throw_projectile" blend="1"/>

