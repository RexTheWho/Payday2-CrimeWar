		<anim name="throw_jav" weight="projectile_jav"/>
		
		<anim name="throw_target" weight="projectile_target"/>
		
		<keys timebase="relative">
			<key at="enter" callback="spawn_grenade" class_name="base"/>
			<key at="trigger" trigger="spawn_grenade" callback="throw_projectile" class_name="base"/>
			<key at="enter" set_extension="anim_data" extension_key="throwing" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="throwing" extension_value="false"/>
			<key at="trigger" trigger="spawn_grenade" set_extension="anim_data" extension_key="throwing" extension_value="false"/>
		</keys>
		<exit name="fps/throw_projectile_exit"/>
	</state>

